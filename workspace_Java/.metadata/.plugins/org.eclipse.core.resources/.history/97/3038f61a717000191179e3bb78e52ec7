package Chapter14;

public class YieldTest {

	static class YieldThread extends Thread {
		String symbol;

		public YieldThread(String symbol) {
			this.symbol = symbol;
		}

		@Override
		public void run() {
			for (int i = 0; i < 60; i++) {
				
				if (i%2==0) {
					System.out.println("symbol");
				}
				
			}
		}

		
		
	}

	
	public static void main(String[] args) {

	}

}
