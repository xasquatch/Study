package Chapter8;

public class DieselSUV {

	private int curX, curY;
	
	public void reportPosition() {
		System.out.println(curX+" "+curY);
	}
	
	public void addFuel() {
		System.out.println("주유소에서 급유");
	}
	
}

class ElectricCar{

	private int curX, curY;
	
	public void reportPosition() {
		System.out.println(curX+" "+curY);
	}
	
	public void addFuel() {
		System.out.println("급속 충전");
	}
	
}




