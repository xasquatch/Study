package Chapter14;

class t1Thread extends Thread {

	@Override
	public void run() {
		for (int i = 1; i < 10; i++) {
			System.out.printf("2¡¿%d=%d%n", i, 2 * i);
		}
		System.out.println(this.getName() + "Á¾·á");
	}

}

public class TestThread {

	public static void main(String[] args) {
		t1Thread t1 = new t1Thread();
		t1.start();

	}

}
