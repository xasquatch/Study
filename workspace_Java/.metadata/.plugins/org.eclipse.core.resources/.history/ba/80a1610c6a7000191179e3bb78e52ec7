			this.dan = dan;
		}

		@Override
		public void run() {
			for (int i = 1; i < 10; i++) {
				result[i-1] = dan+"*"+i+"="+(dan*i);
				
				try {
					
				} catch (InterruptedException e) {
					// TODO: handle exception
				}
			}
			
			
		}
		
		
		
		
	}
	
	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}

}
