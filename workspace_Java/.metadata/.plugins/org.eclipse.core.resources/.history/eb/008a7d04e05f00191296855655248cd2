package Chapter11;

class Product{
	String sn;
	
	public Product(String sn) {
		this.sn = sn;
	}

	@Override
	public int hashCode() {
		return sn.hashCode();
	}

	@Override
	public boolean equals(Object obj) {
		if (this == obj)
			return true;
		if (obj == null)
			return false;
		if (getClass() != obj.getClass())
			return false;
		Product other = (Product) obj;
		if (sn == null) {
			if (other.sn != null)
				return false;
		} else if (!sn.equals(other.sn))
			return false;
		return true;
	}
	
	
}

public class HashCodeTest {

	public static void main(String[] args) {

		String s1 = new String("tq");
		String s2 = new String("tq");
		
		
		System.out.printf("%s   %s",s1.hashCode(),s2.hashCode());
	}

}
